<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="com.Employee" table="Employee">
		<id name="employeeId">
			<generator class="assigned"></generator>
		</id>
		<property name="employeeName"></property>
		<property name="addr"></property>
		<property name="salary"></property>
	</class>
</hibernate-mapping>